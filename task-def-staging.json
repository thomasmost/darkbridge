{
  "family": "teddy-web-task-staging",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "teddy-web-container-staging",
      "memory": 200,
      "cpu": 10,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-ecs-teddy-web-task-staging",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "awslogs-ecs-teddy-web-task-staging"
        }
      },
      "environment": [
        {
          "name": "MYSQL_PORT",
          "value": "3306"
        },
        {
          "name": "MYSQL_USERNAME",
          "value": "web"
        },
        {
          "name": "HOST_DOMAIN",
          "value": "http://staging.callteddy.com"
        },
        {
          "name": "NODE_ENV",
          "value": "staging"
        }
      ],
      "secrets": [
        {
          "name": "EXAMPLE_SECRET_VARIABLE",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:786903541329:secret:EXAMPLE_SECRET_VARIABLE-j7woBv"
        },
        {
          "name": "MYSQL_HOST",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:786903541329:secret:STAGING_MYSQL_HOST-v01lbv"
        },
        {
          "name": "MYSQL_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:786903541329:secret:STAGING_MYSQL_PASSWORD-5gMKpW"
        },
        {
          "name": "IPGEOLOCATION_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:786903541329:secret:STAGING_IPGEOLOCATION_API_KEY-F3rxGf"
        },
        {
          "name": "GOOGLE_CLOUD_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:786903541329:secret:STAGING_GOOGLE_CLOUD_API_KEY-eU5VSu"
        }
      ]
    }
  ],
  "executionRoleArn": "arn:aws:iam::786903541329:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
